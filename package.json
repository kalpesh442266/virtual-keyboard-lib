{
  "name": "reactjs-virtual-keyboard",
  "version": "1.0.1",
  "description": "A customizable virtual keyboard component for React applications with support for multiple layouts, hardware keyboard sync, and touch devices",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "dev": "vite build --watch",
    "lint": "biome check src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "virtual-keyboard",
    "keyboard",
    "reactjs-virtual-keyboard",
    "react",
    "react-keyboard",
    "on-screen-keyboard",
    "touch-keyboard",
    "input",
    "typescript"
  ],
  "author": "HydraFacial",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hydrafacial/virtual-keyboard.git"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0"
  },
  "sideEffects": [
    "*.css"
  ]
}
